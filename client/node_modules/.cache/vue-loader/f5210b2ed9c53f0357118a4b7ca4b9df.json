es/userDataService\"\n\n    export default{\n        name: \"login_page\",\n        data(){\n            return{\n                username: \"\",\n                password: \"\",\n                firstname: \"\",\n                lastname: \"\",\n                data: []\n            }\n        },\n        methods: {\n            userSignin(){\n                userDataService.signInDetail(this.username, this.password, this.firstname,this.lastname)\n                .then(response => {\n                    this.data = response.data;\n                    console.log(this.data)\n                    if(this.data.checked){\n                        alert(this.data.message);\n                        this.$router.push({path: \"/loginSuccess\",query:{data:this.data}})\n                    }else{\n                        alert(this.data.message)\n                    }\n                })\n                .catch(e => {\n                    console.log(e);\n                })\n                \n            }\n        }\n    }\n</script>\n"]}]}