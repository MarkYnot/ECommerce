BA;AAiBA,IAAA,iBAjBA,6BAiBA,KAjBA,EAiBA;AACA,qCAAA,KAAA;AACA,KAnBA;AAoBA,IAAA,cApBA,0BAoBA,KApBA,EAoBA;AACA,kCAAA,KAAA;AACA,KAtBA;AAwBA,IAAA,cAxBA,0BAwBA,KAxBA,EAwBA;AAAA;;AACA,WAAA,WAAA,GAAA,IAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,CAAA;AACA,MAAA,kBAAA,CAAA,aAAA,CAAA,KAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,QAAA,CAAA,IAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,uBAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,MAAA,CAAA,QAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,MAAA,CAAA,QAAA,CAAA,IAAA,CAAA,CAAA,EAAA,KAAA;AACA,OANA,EAOA,KAPA,CAOA,UAAA,CAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,CAAA;AACA,OATA;AAUA;AArCA,GAVA;AAiDA,EAAA,OAjDA,qBAiDA;AACA,SAAA,gBAAA;AACA;AAnDA,CAAA","sourcesContent":["\n<!-- This Page contains Home State and Item State -->\n<template>\n  <div v-if=\"!detailState\">\n    <div id=\"totalContainer\">\n      <div id=\"soldOut\">\n        <div>\n          <h1>Sold out soon</h1>\n        </div>\n        <div v-for=\"(item,index) in lowStock\" :key=\"index\" :id=\"lowStockClassName(index)\" class=\"stock\">\n          <p class=\"title\">{{ item.title }}</p>\n          <img :src=\"'/static/'+item.image\" alt=\"\" class=\"img1\">\n          <p class=\"price\"> Price: {{ item.price }}$</p>\n          <button @click=\"getItemDetails(item.title)\">View Phone list</button>\n        </div>\n      </div>\n      <div id=\"BestSell\">\n        <div>\n          <h1>Best Sell</h1>\n        </div>\n        <div v-for=\"(item,index) in bestSell\" :key=\"index\" :id=\"bestSellClassName(index)\" class=\"sell\">\n          <p class=\"title\">{{ item.sell.title }}</p>\n          <img :src=\"'/static/'+item.sell.image\" alt=\"\" class=\"img2\">\n          <p class=\"rating\">Average_Rating: {{ item.avg_rate }}</p>\n          <button @click=\"getItemDetails(item.sell.title)\">View Phone list</button>\n        </div>\n      </div>\n    </div>\n  </div>\n\n\n  <!-- Switch to Item State -->\n  <div v-else-if=\"detailState\">\n    <h1>DetailState</h1>\n    <div id=\"page-wrap\">\n      <div id=\"img-wrap\">\n        <img :src=\"'/static/'+testItem.item.image\">\n      </div>\n      <div id=\"item-details\">\n        <h3>{{testItem.item.title}}</h3>\n        <h3>Brand: {{testItem.item.brand}}</h3>\n        <h3>Price: {{testItem.item.price}}</h3>\n        <h3>Stock: {{testItem.item.stock}}</h3>\n        <button id=\"add-to-cart\">Add to Cart</button>\n      </div>\n    </div>\n  </div>\n\n\n</template>\n\n\n\n<script>\nimport ProductDataService from \"../services/ProductDataService\";\nexport default {\n  name: \"HomePageList\",\n  data() {\n    return {\n      lowStock: [],\n      bestSell: [],\n      detailState: false,\n      testItem: []\n    };\n  },\n  methods: {\n    async getHomeStateData() {\n      try{\n        let data = await ProductDataService.getBestAndStockPhone()\n        this.lowStock = data.data.stock\n        this.bestSell = data.data.sell\n        console.log(this.lowStock)\n      }catch(err){\n        console.log(err);\n      }\n    },\n    setWebStorage(item){\n      this.$router.push({name:'productDetail',query:{id: item}})\n    },\n    lowStockClassName(index){\n      return `lowStock_item-${index}`\n    },\n    bestSellClassName(index){\n      return `bestSell_item-${index}`\n    },\n    photoClassName(index){\n      return `photo_item-${index}`\n    },\n    \n    getItemDetails(title) {\n      this.detailState = true;\n    console.log(1)\n      ProductDataService.getItemDetail(title)\n      .then(response => {\n        this.testItem = response.data;\n        console.log(\"get item successfully\");\n        console.log(this.testItem);\n        console.log(this.testItem.item[0].brand);\n      })\n      .catch(e => {\n        console.log(e);\n      })\n    }\n  },\n  mounted() {\n    this.getHomeStateData();\n  },\n};\n</script>\n\n<style>\n\n.viewBtn{\n  margin-bottom: 5px;\n}\n\n.title{\n  border-bottom: 1px solid grey;\n}\n\n.price{\n  border-top: 1px solid grey;\n  padding-top: 7px;\n  padding-bottom: 7px;\n  font-size: 15px;\n  border-bottom: 1px solid grey ;\n}\n\n.rating{\n  border-top: 1px solid grey;\n  font-size: 15px;\n  padding-top: 7px;\n  padding-bottom: 7px;\n  border-bottom: 1px solid grey ;\n}\n\n#totalContainer {\n  display: grid;\n  grid-template-rows: 1;\n  grid-template-columns: 40% 40%;\n  grid-column-gap: 5%;\n  max-width: 90%;\n  margin-left: 10%;\n  margin-top: 5%;\n}\n\n#soldOut {\n  grid-row: 1;\n  grid-column: 1;\n  border: 1px solid black;\n  display: grid;\n  grid-template-rows: auto;\n  grid-row-gap: 5px;\n}\n\n#BestSell {\n  grid-row: 1;\n  grid-column: 2;\n  border: 1px solid black;\n  display: grid;\n  grid-template-rows: auto;\n  grid-row-gap: 5px;\n}\n\n.stock{\n  border: 1px solid grey;\n  width: 90%;\n  margin-left: 5%;\n  margin-top: 5%;\n  margin-bottom: 5%;\n  display: grid;\n  grid-template-rows:auto;\n  height: auto;\n}\n\n.sell{\n  border: 1px solid grey;\n  width: 90%;\n  margin-left: 5%;\n  margin-top: 5%;\n  margin-bottom: 5%;\n  display: grid;\n  grid-template-rows: auto;\n  height: auto;\n}\n\n.img1{\n  grid-row: 2;\n  width: 50%;\n  height: 55%;\n}\n\n.img2{\n  grid-row: 2;\n  width: 50%;\n  height: 55%;\n}\n</style>\n"],"sourceRoot":"src/components"}]}