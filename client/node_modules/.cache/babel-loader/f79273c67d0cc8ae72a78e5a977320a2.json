            <input class=\"textInput\" type = \"text\" v-model=\"stock\"><br>\n          <br>\n           \n              \n          <button class=\"textInput\" type=\"button\" @click=\"addPage\">Add</button>\n  </div>\n</template>\n\n<script>\nimport ProductDataService from \"../services/ProductDataService\"\n  export default {\n\n    data() {\n    return {\n               title:\"\",\n               price:'',\n               brand:\"\",\n               image:\"\",\n               userid:\"\",\n               stock:''         \n   }\n  },\n\n  methods:{\n     async addPage(){\n       if(this.title != \"\" && this.price != \"\" && this.brand != \"\" && this.stock != \"\" && this.image != \"\"){\n      this.userid = this.$cookies.get('userid');\n        var response = await ProductDataService.addListing(this.title, this.price, this.brand, this.image, this.stock, this.userid)\n        var res= response.data;\n        \n        if(res.status == \"1\"){\n          alert(res.msg)\n        }else{\n\n           alert(\"you have successfully added\");\n          //  this.$router.go(0);\n\n        }\n     }else{\n        alert(\"please fullfill all the infomation\")\n\n     }\n    },\n\n    handlePreview(file){\n     console.log(file)\n    },\n\n\n    handleExceed(file){\n\n      this.$message.warning(`only one file should be chosen`)\n    },\n       \n    uploadSucess(response,file,fileList){\n        if(response.flag ==true){\n         this.$message.warning(`uploaded success`)\n         return;\n         }\n      }\n      \n\n  }\n    \n  }\n</script>\n\n<style>\n.textInput{\n     position: relative;\n     right: -190px\n}\n\n\n.upload{\n     position: relative;\n     right: -190px;\n     top: -8px;\n     width:300px;\n}\n</style>"],"sourceRoot":"src/components"}]}